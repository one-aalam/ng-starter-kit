    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-preset-angular": "^11.0.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "tailwindcss": "^2.2.19",
    "typescript": "~4.4.3"
